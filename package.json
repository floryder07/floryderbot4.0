{
  "name": "floryderbot4.0",
  "version": "0.1.0",
  "description": "NBA analytics & learning Discord bot (FloryderBot 4.0)",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "register-commands": "ts-node src/registerCommands.ts",
    "lint": "eslint \"src/**/*.{ts,js}\" --ext .ts",
    "lint:fix": "eslint \"src/**/*.{ts,js}\" --ext .ts --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "test": "jest"
  },
  "author": "floryder07",
  "license": "MIT",
  "dependencies": {
    "@discordjs/rest": "^1.3.0",
    "@prisma/client": "^5.8.0",
    "discord-api-types": "^0.37.44",
    "discord.js": "^14.11.0",
    "dotenv": "^16.3.1",
    "pino": "^8.12.0"
  },
  "devDependencies": {
    "@types/node": "^20.5.6",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.1.4",
    "jest": "^29.6.0",
    "prettier": "^3.0.0",
    "prisma": "^5.8.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}
